import java.util.Scanner;

public class Gertrude {
    private String daterName;
    private int daterAge;
    private int daterCharisma;
    private String daterEyes;

    public Gertrude(String name, int age, int charisma, String eyes) {
        daterName = name;
        daterAge = age;
        daterCharisma = charisma;
        daterEyes = eyes;
    }

    public void introduce() {
        System.out.println("Hi, My name is " + daterName + ".\nI am " + daterAge + " years old.");
        System.out.println("I think my charisma level is " + daterCharisma + ".\nMy eyes are " + daterEyes + ".");
    }
}
